			(* OpenBSD 7.8 /usr/include/sys/stat.h *)
				st_mode*: mode_t;
				st_dev*: dev_t;
				st_ino*: ino_t;
				st_nlink*: nlink_t;
				st_uid*: uid_t;
				st_gid*: gid_t;
				st_rdev*: dev_t;

				st_atim*: timespec_t;
				st_mtim*: timespec_t;
				st_ctim*: timespec_t;

				st_size*: off_t;
				st_blocks*: blkcnt_t;
				st_blksize*: blksize_t;
				st_flags*: uint32_t;
				st_gen*: uint32_t;

				__st_birthtim*: timespec_t;